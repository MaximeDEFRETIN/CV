/* ----- compagnon (extends box) ----- */
.compagnon {position:relative;}
.compagnon .inner {
	border:2px solid #CDA261;
	border-radius:10px;
	color:#333;
	background-color:#fcfcfc;
	background-image:url(../local/cache-gd2/fd/7e45fe9bd22d3579c28995877bb448.png?1582554476);
	background-position: -10px -10px;
	background-repeat: no-repeat;
	
}
.compagnon .inner .hd {padding-left:60px;}
.compagnon .inner .bd {padding-left:60px;}
.compagnon b{}
.compagnon .act {
	background-color: transparent;
	border-top: none;
}
.compagnon .ft {
	padding: 0;
}
.compagnon button.submit {
	font-size:100%;
	border:2px solid #CDA261;
	border-top-width:2px;
	border-left-width:2px;
	border-right-width:0px;
	border-bottom-width:0px;
	border-top-left-radius:9px;
	border-top-right-radius:0;
	border-bottom-right-radius:9px;
	border-bottom-left-radius:0;
	background: #f3e8d8;
	padding:3px 4px 4px 3px;
	cursor:pointer;
	margin:0;
	color: #444;
}

.compagnon .target {
	position:absolute; bottom:5px; left:5px;
	display:block; width:32px; height:32px;
	background:url(../local/cache-gd2/fc/f376bb434b4d18c4280703c3e0cc32.png?1582554476);
	cursor:crosshair;
}
#navigation .compagnon .inner .hd {min-height:2.5em;}
#navigation .compagnon .inner .bd {padding-left:10px;}
